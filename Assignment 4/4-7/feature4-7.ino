String multiplication(int multiple) {
  String result;
  result = String(multiple) + " * 1  = " + String(multiple * 1) + "\n" +
           String(multiple) + " * 2  = " + String(multiple * 2) + "\n" +
           String(multiple) + " * 3  = " + String(multiple * 3) + "\n" +
           String(multiple) + " * 4  = " + String(multiple * 4) + "\n" +
           String(multiple) + " * 5  = " + String(multiple * 5) + "\n" +
           String(multiple) + " * 6  = " + String(multiple * 6) + "\n" +
           String(multiple) + " * 7  = " + String(multiple * 7) + "\n" +
           String(multiple) + " * 8  = " + String(multiple * 8) + "\n" +
           String(multiple) + " * 9  = " + String(multiple * 9) + "\n" +
           String(multiple) + " * 10 = " + String(multiple * 10) + "\n" +
           String(multiple) + " * 11 = " + String(multiple * 11) + "\n" +
           String(multiple) + " * 12 = " + String(multiple * 12);

  return result;
}
